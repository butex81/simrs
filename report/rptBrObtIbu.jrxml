	<queryString>
		<![CDATA[SELECT
     tgl_perawatan,
     jam,
     detail_pemberian_obat_ibu.`no_rawat`,
     reg_periksa_ibu.`no_rm_ibu`,
     nm_pasien,
     detail_pemberian_obat_ibu.`kd_penyakit`,
     nm_penyakit,
     diagnosa,
     detail_pemberian_obat_ibu.`kd_obat`,
     nm_obat,
     
     tambahan,jml,biaya_obat,total
FROM
     `detail_pemberian_obat_ibu` detail_pemberian_obat_ibu,
     `reg_periksa_ibu` reg_periksa_ibu,
     `pasien_ibu` pasien_ibu,
     `penyakit` penyakit,
     `obat` obat
WHERE
     detail_pemberian_obat_ibu.no_rawat = reg_periksa_ibu.no_rawat
     and reg_periksa_ibu.no_rm_ibu = pasien_ibu.no_rm_ibu
     and detail_pemberian_obat_ibu.kd_penyakit = penyakit.kd_penyakit
     and detail_pemberian_obat_ibu.kd_obat = obat.kd_obat
ORDER BY
     detail_pemberian_obat_ibu.tgl_perawatan ASC]]>
	</queryString>
