	<queryString>
		<![CDATA[SELECT
     rawat_inap_pr_bayi.`no_rawat`,
     reg_periksa_bayi.`no_rm_bayi`,
     nm_pasien,
     nm_perawatan,
     rawat_inap_pr_bayi.`nip`,
     nama,
     suhu_tubuh,
     tensi,
     hasil,
     perkembangan,
     tgl_perawatan,
     jam_rawat,
     biaya_rawat
FROM
     `pasien_bayi` pasien_bayi,
     `reg_periksa_bayi` reg_periksa_bayi,
     `jns_perawatan` jns_perawatan,
     `petugas` petugas,
     `rawat_inap_pr_bayi` rawat_inap_pr_bayi
WHERE
     rawat_inap_pr_bayi.no_rawat = reg_periksa_bayi.no_rawat
     and reg_periksa_bayi.no_rm_bayi = pasien_bayi.no_rm_bayi
     and rawat_inap_pr_bayi.kd_jenis_prw = jns_perawatan.kd_jenis_prw
     and rawat_inap_pr_bayi.nip = petugas.nip
ORDER BY
     rawat_inap_pr_bayi.no_rawat ASC]]>
	</queryString>
