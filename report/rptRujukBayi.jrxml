	<queryString>
		<![CDATA[SELECT
     no_rujuk,
     rujuk_bayi.`no_rawat`,
     reg_periksa_bayi.`no_rm_bayi`,
     nm_pasien,
     rujuk_ke,
     tgl_rujuk,
     keterangan_diagnosa,
     rujuk_bayi.`kd_dokter`,
     nm_dokter
FROM
     `rujuk_bayi` rujuk_bayi,
     `reg_periksa_bayi` reg_periksa_bayi,
     `pasien_bayi` pasien_bayi,
     `dokter` dokter
WHERE
     rujuk_bayi.no_rawat = reg_periksa_bayi.no_rawat
     and reg_periksa_bayi.no_rm_bayi = pasien_bayi.no_rm_bayi
     and rujuk_bayi.kd_dokter = dokter.kd_dokter
