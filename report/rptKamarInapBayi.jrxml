	<queryString>
		<![CDATA[SELECT
     kamar_inap_bayi.`no_rawat`,
     reg_periksa_bayi.`no_rm_bayi`,
     pasien_bayi.`nm_pasien`,
     kamar_inap_bayi.`kd_kamar`,
     bangsal.`nm_bangsal`,
     kamar.`trf_kamar`,
     kamar_inap_bayi.`diagnosa_awal`,
     kamar_inap_bayi.`cara_masuk`,
     kamar_inap_bayi.`tgl_masuk`,
     kamar_inap_bayi.`jam_masuk`,
     kamar_inap_bayi.`tgl_keluar`,
     kamar_inap_bayi.`jam_keluar`,
     kamar_inap_bayi.`ttl_biaya`
FROM
     `kamar_inap_bayi` kamar_inap_bayi,
     `reg_periksa_bayi` reg_periksa_bayi,
     `pasien_bayi` pasien_bayi,
     `kamar` kamar,
     `bangsal` bangsal
WHERE
     kamar_inap_bayi.no_rawat = reg_periksa_bayi.no_rawat
     and reg_periksa_bayi.no_rm_bayi = pasien_bayi.no_rm_bayi
     and kamar_inap_bayi.kd_kamar = kamar.kd_kamar
     and kamar.kd_bangsal = bangsal.kd_bangsal
ORDER BY
     kamar_inap_bayi.no_rawat ASC]]>
