     resep_obat_bayi.`kd_obat`,
     nm_obat,
     dosis_obat
FROM
     `resep_obat_bayi` resep_obat_bayi,
     `reg_periksa_bayi` reg_periksa_bayi,
     `pasien_bayi` pasien_bayi,
     `penyakit` penyakit,
     `obat` obat
